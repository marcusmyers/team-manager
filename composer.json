{
    "name": "marcusmyers/team-manager",
    "description": "A Laravel package to manage a Team of Athletes",
    "type": "library",
    "license": "MIT",
    "version": "1.0.0",
    "authors": [
        {
            "name": "Mark Myers",
            "email": "marcusmyers@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
