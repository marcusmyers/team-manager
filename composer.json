{
    "name": "marcusmyers/team-manager",
    "description": "A Laravel package to manage a Team of Athletes",
    "type": "library",
    "license": "MIT",
    "version": "3.0.0",
    "authors": [
        {
            "name": "Mark Myers",
            "email": "marcusmyers@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Marcusmyers\\TeamManager\\": "src/",
            "Marcusmyers\\TeamManager\\Database\\Factories\\": "database/factories/",
            "Marcusmyers\\TeamManager\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Marcusmyers\\TeamManager\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0|^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^6.0|^7.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Marcusmyers\\TeamManager\\TeamManagerServiceProvider"
            ]
        }
    }
}
